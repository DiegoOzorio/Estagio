Algoritmo "Aula3_3"
// Função :
// Autor :
// Data : 10/05/2022
// Seção de Declarações
var
    N1, N2: Inteiro
    M: Real
inicio
// Seção de Comandos
      Escreva ("Informe um numero: ")
      Leia (N1)
      Escreva ("Informe outro numero: ")
      Leia (N2)
      M <- (N1 + N2)/2
      Escreva("A Media entre ", N1, " e ", N2, " e igual a ", M)

fimalgoritmo
