Algoritmo "Aula3_2"
// Função :
// Autor :
// Data : 10/05/2022
// Seção de Declarações
var
    N1, N2, S: Inteiro
inicio
// Seção de Comandos
      Escreva ("Informe um numero: ")
      Leia (N1)
      Escreva ("Informe outro numero: ")
      Leia (N2)
      S <- N1 + N2
      Escreva("A some entre ", N1, " e ", N2, " e igual a ", S)
      
fimalgoritmo
