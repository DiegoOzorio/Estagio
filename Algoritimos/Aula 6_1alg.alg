algoritmo "Aula6_1"
// Função :
// Autor :
// Data : 11/05/2022
// Seção de Declarações 
var

     ANO, NAS_ANO, IDADE : INTEIRO
     
inicio
// Seção de Comandos 
   Escreval("Qual o Ano que vc nasceu?: ")
   Leia(NAS_ANO)
   Escreval("Qual o Ano que estamos no momento?: ")
   Leia(ANO)
   IDADE <- ANO - NAS_ANO
   Escreva("Sua Idade e: ",IDADE)

fimalgoritmo
