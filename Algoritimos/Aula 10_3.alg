algoritmo "Aula10_4"
// Função :
// Autor :
// Data : 30/05/2022
// Seção de Declarações
var
Cont, N, R: Inteiro
inicio
// Seção de Comandos
Cont <- 1
Escreval("Tabuada de qual numero?:")
leia(N)
Repita
  R <- N * Cont
  Escreval(N," x ",Cont," = ", R)
  Cont <- Cont + 1
Ate (Cont > 10)


fimalgoritmo
