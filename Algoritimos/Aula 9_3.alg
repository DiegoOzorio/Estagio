// Seção de Declarações
var
 cont, salto, valor: inteiro
inicio
cont <- 0
 Escreval("Quer contar ate quanto")
 leia(valor)
  Escreval("Qual o valoe do salto")
 leia(salto)
 
Enquanto (cont <= valor) faca
     Escreval(cont)
     cont <- cont + salto
fimenquanto
  Escreval("Terminei de contar")

// Seção de Comandos
fimalgoritmo
