// Seção de Declarações
var

     DReais, Ddolar, DConv : Real

inicio
// Seção de Comandos
   Escreval("Quanstos Reais vc tem?: ")
   Leia(DReais)
   Escreval("Cotação do Dolar atual 5,11")
   Ddolar <- 5,11
   Dconv <- DReais / Ddolar
   Escreva("A COEVERSÃO DA QUANTIA EM DOLARES E : ", Dconv)

fimalgoritmo
