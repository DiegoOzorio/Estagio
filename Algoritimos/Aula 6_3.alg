algoritmo "Aula6_3"
// Função :
// Autor :
// Data : 12/05/2022
// Seção de Declarações 
var
     Celcios, F : Real
inicio
// Seção de Comandos 
    Escreva("Qual sua tenperatura aqui?: ")
    Leia(F)
    Celcios <- (F - 32) / 1.8
    Escreva("Essa temperatura em °C sera de : ", Celcios:4:1 )

fimalgoritmo
